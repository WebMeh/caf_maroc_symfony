{{ form_start(form) }}
<div class="row mb-3">
	<div class="col">
		<label>{{ form_label(form.nom) }}</label>
		{{ form_widget(form.nom, {'attr': {'class': 'form-control'}}) }}
	</div>
	<div class="col">
		<label>{{ form_label(form.ville) }}</label>
		{{ form_widget(form.ville, {'attr': {'class': 'form-control'}}) }}
	</div>
</div>
<div class="row mb-3 ">
	<div class="col">
		<label>{{ form_label(form.capacite) }}</label>
		{{ form_widget(form.capacite, {'attr': {'class': 'form-control'}}) }}
	</div>
	<div class="col"></div>
</div>

{{ form_widget(form) }}
<div class="d-flex justify-content-end mt-3 login_container">
	<button name="button" class="btn btn-success" >Enregistrer</button>
</div>
{{ form_end(form) }}
